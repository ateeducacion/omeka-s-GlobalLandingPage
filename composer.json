{
    "name": "ateeducacion/omeka-s-globallandingpage",
    "description": "Deliver a self-contained landing page for the Omeka S global index without relying on public sites or themes",
    "type": "omeka-s-module",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Area de Tecnologia Educativa",
            "email": "ate.educacion@gobiernodecanarias.org"
        }
    ],
    "homepage": "https://github.com/ateeducacion/omeka-s-GlobalLandingPage",
    "support": {
        "issues": "https://github.com/ateeducacion/omeka-s-GlobalLandingPage/issues"
    },
    "keywords": [
        "Omeka S",
        "module",
        "landing page",
        "standalone override"
    ],
    "require": {
        "php": ">=7.4"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "omeka/composer-addon-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "*",
        "zerocrates/extract-tagged-strings": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "GlobalLandingPage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GlobalLandingPageTest\\": "test/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zerocrates/extract-tagged-strings.git"
        }
    ]
}
